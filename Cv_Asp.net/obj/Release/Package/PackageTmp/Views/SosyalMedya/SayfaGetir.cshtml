
@model Cv_Asp.net.Models.Entity.Tbl_SosyalMeyda
@{
    ViewBag.Title = "Sosyal Medya Düzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4> Sosyal Medya Düzenleme Sayfası</h4>
<form method="post">
    <br />
    <br />

    @Html.Label("Sosyal Medya Adı")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("İkon")
    @Html.TextBoxFor(x => x.İkon, new { @class = "form-control" })
    <br />

    <button class="btn btn-info">Güncelle</button>
</form>



