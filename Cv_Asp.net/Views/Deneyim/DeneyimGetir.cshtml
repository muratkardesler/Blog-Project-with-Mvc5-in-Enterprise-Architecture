@model Cv_Asp.net.Models.Entity.Tbl_Deneyimler
@{
    ViewBag.Title = "DeneyimGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form method="post">
    <br />
    <br />

    @Html.Label("Deneyim ID")
    @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
    <br />

    @Html.Label("Başlık")
    @Html.TextBoxFor(x => x.Baslık, new { @class = "form-control" })

    <br />

    @Html.Label("Alt Başlık")
    @Html.TextBoxFor(x => x.AltBaslık, new { @class = "form-control" })

    <br />

    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })

    <br />

    @Html.Label("Açıklama")
    @Html.TextAreaFor(x => x.Acıklama, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Güncelle</button>
</form>

